.logo-list-container {
  padding-top: 4rem;
  padding-bottom: 4rem;
  .container {
    margin: auto;
  }

  .logo-list-count {
    width: 100%;
    max-width: 650px;
    margin: auto;
  }

  .logo-with-text {
    width: 100%;

  }

  .logo-container {
    display: block;
    width: 100%;
    padding-top: 100%;
    position: relative;

    border: 3px solid #e4d0d1;
    overflow: hidden;
    background-color: white;
    border-radius: 50%;
  }

  .logo-image-container {
    width: 92.5%;
    height: 92.5%;

    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);

    border-radius: 50%;
    overflow: hidden;

  }

  .logo-img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center;
  }

  .logo-title {
    margin-top: 0.5rem;
    color: #c16452;
    display: block;
    text-align: center;
    font-size: 1.2rem;
    font-weight: 600;
  }
}
